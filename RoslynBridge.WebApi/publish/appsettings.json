{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "RoslynBridge.WebApi": "Information"
    },
    "EventLog": {
      "SourceName": "Roslyn Bridge Web API",
      "LogName": "Application",
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "RoslynBridge": {
    "BaseUrl": "http://localhost:59123",
    "TimeoutSeconds": 30
  },
  "History": {
    "MaxEntries": 1000,
    "Enabled": true
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  }
}
